{
  "$schema": "https://schemastore.azurewebsites.net/schemas/json/tsconfig.json",
  "compilerOptions": {
    "baseUrl": "./src",
    "module": "CommonJS",
    "importHelpers": true,
    "outDir": "v2ray-ng/v2ray-ng/main",
    "target": "es2018",
    "lib": ["es2018", "esnext"],
    "sourceMap": true,
    "declaration": false,
    "downlevelIteration": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "forceConsistentCasingInFileNames": true,
    "moduleResolution": "node",
    "experimentalDecorators": true,
    "resolveJsonModule": true,
    "isolatedModules": true,
    "paths": {
      "@typing/*": ["./typings/*"],
      "@main/*": ["./main/*"],
      "@renderer/*": ["./renderer/app/*"]
    }
  },
  "files": ["./src/main/main.ts", "./src/config.ts"],
  "include": ["src/main", "src/environments", "src/typings", "src/renderer/app/interfaces/form-schema.interface.ts"],
  "exclude": ["src/renderer", "./node_modules"]
}
